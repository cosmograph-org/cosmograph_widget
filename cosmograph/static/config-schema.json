{
  "$ref": "#/definitions/configSchema",
  "definitions": {
    "configSchema": {
      "type": "object",
      "properties": {
        "disable_simulation": {
          "anyOf": [
            {
              "type": "boolean"
            },
            {
              "type": "object"
            }
          ],
          "default": null
        },
        "show_dynamic_labels": {
          "type": "boolean",
          "default": true
        },
        "show_top_labels": {
          "type": "boolean",
          "default": false
        },
        "show_top_labels_limit": {
          "type": "number",
          "default": 100
        },
        "show_top_labels_value_key": {
          "type": "string"
        },
        "show_hovered_node_label": {
          "type": "boolean",
          "default": false
        },
        "node_label_accessor": {
          "type": "string"
        },
        "node_label_class_name": {
          "type": "string"
        },
        "node_label_color": {
          "type": "string"
        },
        "hovered_node_label_class_name": {
          "type": "string"
        },
        "hovered_node_label_color": {
          "type": "string"
        },
        "simulation_decay": {
          "type": "number",
          "default": 1000
        },
        "simulation_gravity": {
          "type": "number",
          "default": 0
        },
        "simulation_center": {
          "type": "number",
          "default": 0
        },
        "simulation_repulsion": {
          "type": "number",
          "default": 0.1
        },
        "simulation_repulsion_theta": {
          "type": "number",
          "default": 1.7
        },
        "simulation_repulsion_quadtree_levels": {
          "type": "number",
          "default": 12
        },
        "simulation_link_spring": {
          "type": "number",
          "default": 1
        },
        "simulation_link_distance": {
          "type": "number",
          "default": 2
        },
        "simulation_link_dist_random_variation_range": {
          "type": "array",
          "minItems": 2,
          "maxItems": 2,
          "items": [
            {
              "type": "number"
            },
            {
              "type": "number"
            }
          ],
          "default": [
            1,
            1.2
          ]
        },
        "simulation_repulsion_from_mouse": {
          "type": "number",
          "default": 2
        },
        "simulation_friction": {
          "type": "number",
          "default": 0.85
        },
        "background_color": {
          "type": "string",
          "default": "#222222"
        },
        "space_size": {
          "type": "number",
          "default": 4096
        },
        "node_color": {
          "type": "string",
          "default": "#b3b3b3"
        },
        "node_greyout_opacity": {
          "type": "number",
          "default": 0.1
        },
        "node_size": {
          "type": "number",
          "default": 4
        },
        "node_size_scale": {
          "type": "number",
          "default": 1
        },
        "render_highlighted_node_ring": {
          "type": "boolean",
          "default": true
        },
        "render_hovered_node_ring": {
          "type": "boolean",
          "default": true
        },
        "hovered_node_ring_color": {
          "type": "string",
          "default": "white"
        },
        "focused_node_ring_color": {
          "type": "string",
          "default": "white"
        },
        "render_links": {
          "type": "boolean",
          "default": true
        },
        "link_color": {
          "type": "string",
          "default": "#666666"
        },
        "link_greyout_opacity": {
          "type": "number",
          "default": 0.1
        },
        "link_width": {
          "type": "number",
          "default": 1
        },
        "link_width_scale": {
          "type": "number",
          "default": 1
        },
        "curved_links": {
          "type": "boolean",
          "default": false
        },
        "curved_link_segments": {
          "type": "number",
          "default": 19
        },
        "curved_link_weight": {
          "type": "number",
          "default": 0.8
        },
        "curved_link_control_point_distance": {
          "type": "number",
          "default": 0.5
        },
        "link_arrows": {
          "type": "boolean",
          "default": true
        },
        "link_arrows_size_scale": {
          "type": "number",
          "default": 1
        },
        "link_visibility_distance_range": {
          "type": "array",
          "minItems": 2,
          "maxItems": 2,
          "items": [
            {
              "type": "number"
            },
            {
              "type": "number"
            }
          ],
          "default": [
            50,
            150
          ]
        },
        "link_visibility_min_transparency": {
          "type": "number",
          "default": 0.25
        },
        "use_quadtree": {
          "type": "boolean",
          "default": false
        },
        "pixel_ratio": {
          "type": "number",
          "default": 2
        },
        "scale_nodes_on_zoom": {
          "type": "boolean",
          "default": true
        },
        "initial_zoom_level": {
          "type": "number",
          "default": 1
        },
        "disable_zoom": {
          "type": "boolean",
          "default": false
        },
        "fit_view_on_init": {
          "type": "boolean",
          "default": true
        },
        "fit_view_delay": {
          "type": "number",
          "default": 250
        },
        "fit_view_by_nodes_in_rect": {
          "type": "array",
          "minItems": 2,
          "maxItems": 2,
          "items": [
            {
              "type": "array",
              "minItems": 2,
              "maxItems": 2,
              "items": [
                {
                  "type": "number"
                },
                {
                  "type": "number"
                }
              ]
            },
            {
              "type": "array",
              "minItems": 2,
              "maxItems": 2,
              "items": [
                {
                  "type": "number"
                },
                {
                  "type": "number"
                }
              ]
            }
          ]
        },
        "random_seed": {
          "type": [
            "number",
            "string"
          ]
        },
        "node_sampling_distance": {
          "type": "number",
          "default": 150
        }
      },
      "additionalProperties": false
    }
  },
  "$schema": "http://json-schema.org/draft-07/schema#"
}